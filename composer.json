{
  "name": "madewithlove/elasticsearcher-laravel",
  "description": "Elasticsearcher provider for Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "elasticsearch",
    "elastic"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/special-elektronik/elasticsearcher"
    }
  ],
  "require": {
    "madewithlove/elasticsearcher": "dev-0.7-dev"
  },
  "autoload": {
    "psr-4": {
      "Madewithlove\\ElasticSearcherLaravel\\": [
        "src"
      ]
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Madewithlove\\ElasticSearcherLaravel\\ServiceProvider"
        ]
    }
  }
}